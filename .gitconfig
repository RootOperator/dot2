[init]
	defaultBranch = master
[credential]
	helper = store
[user]
	email = 20143055+RootOperator@users.noreply.github.com
	name = RootOperator
[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true
    side-by-side = true
    line-numbers = true
    syntax-theme = "kanagawa"

[merge]
    conflictstyle = zdiff3

[diff]
    colorMoved = default
